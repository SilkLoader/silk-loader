<configuration>

    <appender name="DEFAULT_CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d{HH:mm:ss}] [%level] [%logger]: %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="Silk Loader" level="INFO"/>
    <root level="INFO"> <appender-ref ref="DEFAULT_CONSOLE" />
    </root>

</configuration>